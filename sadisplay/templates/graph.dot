/*
 Incorporating fixes from
 https://spin.atomicobject.com/2017/11/15/table-rel-diagrams-graphviz/
*/
digraph G {
    fontname = "Fira Code Regular"
    fontsize = 8
    rankdir=LR

    node [
        fontname = "Fira Code Regular"
        fontsize = 8
        shape = "plaintext"
    ]

    edge [
        fontname = "Fira Code Regular"
        fontsize = 8
    ]
{{ template }}
}
